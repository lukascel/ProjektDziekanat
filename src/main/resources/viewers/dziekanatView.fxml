<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane onKeyPressed="#loginKeyAction" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.pwn.reaktor.dziekanat.controller.DziekanatController">
   <children>
      <Label fx:id="lblLogin" layoutX="115.0" layoutY="38.0" text="Login:" />
      <TextArea fx:id="taLogin" layoutX="115.0" layoutY="55.0" prefHeight="38.0" prefWidth="149.0" promptText="login:" />
      <Label fx:id="lblPassword" layoutX="118.0" layoutY="128.0" text="Password" />
      <PasswordField fx:id="psPassword" layoutX="115.0" layoutY="145.0" promptText="password" />
      <Button fx:id="btShow" layoutX="289.0" layoutY="145.0" mnemonicParsing="false" onMouseClicked="#showPassEvent" text="show" />
      <Button fx:id="btLogin" layoutX="106.0" layoutY="188.0" mnemonicParsing="false" onMouseClicked="#loginEvent" text="LOGIN" />
      <Button fx:id="btGuest" layoutX="189.0" layoutY="188.0" mnemonicParsing="false" onMouseClicked="#guestEvent" text="GUEST" />
      <Button fx:id="btSignIn" layoutX="274.0" layoutY="188.0" mnemonicParsing="false" onMouseClicked="#signInEvent" text="SIGN IN" />
      <TextField fx:id="tfPassword" layoutX="115.0" layoutY="145.0" promptText="password:" visible="false" />
   </children>
</AnchorPane>
